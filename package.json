{
  "name": "finances-management-app",
  "version": "0.1.0",
  "description": "My finances management app, built with nextjs and ant design.",
  "main": "src/pages/index.jsx",
  "repository": "https://github.com/jsi1v4/finances-management-app.git",
  "author": "Jsi1v4 <me@jsi1v4.dev>",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "eslint . --fix --ext jsx --ext js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "commitlint --edit",
      "pre-push": "yarn test:ci"
    }
  },
  "lint-staged": {
    "*.@(js|jsx)": [
      "yarn lint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.0.18",
    "@ant-design/icons": "4.2.1",
    "@next/bundle-analyzer": "^9.1.4",
    "antd": "4.3.0",
    "axios": "^0.21.1",
    "next": "^10.0.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.13.1",
    "styled-components": "^5.0.0",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/react": "^11.2.5",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "browserslist": "> 0.25%, not dead",
  "jest": {
    "setupFiles": [
      "<rootDir>/setup-tests.js"
    ]
  }
}
